{
  "name": "react_repo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:server": "node server/index.js",
    "start": "pnpm start:server & pnpm dev",
    "optimize:images": "node src/scripts/optimize-images.cjs",
    "build": "pnpm run optimize:images && tsc -b && vite build",
    "prerender": "node scripts/prerender.cjs",
    "build:seo": "pnpm run build && pnpm run prerender",
    "optimize:videos": "node src/scripts/optimize-videos.cjs ./raw-videos ./public/videos --crf 23 --preset slow --width 1080",
    "lint": "eslint .",
    "preview": "vite preview"
    },
  "reactSnap": {
    "include": [
      "/"
    ],
    "skipThirdPartyRequests": true,
    "source": "dist",
    "destination": "dist",
    "removeBlobs": true,
    "inlineCss": false,
    "cacheAjaxRequests": true,
    "fixWebpackChunksIssue": false,
    "puppeteer": {
      "cache": false
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
